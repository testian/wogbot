package modules;

import org.schwering.irc.lib.IRCUser;

import core.Bot;
import core.BotCommand;
import core.BotException;

public class ModeCommand implements BotCommand {

	public String getUsage() {
		// TODO Auto-generated method stub
		return null;
	}

	public void execute(Bot bot, String channel, IRCUser user, String message)
			throws BotException {
		bot.assertAdmin(user);
		bot.getConnection().doMode(channel,message);

	}

	public String getName() {
		// TODO Auto-generated method stub
		return "mode";
	}

}
